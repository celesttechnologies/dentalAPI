{
    "name": "wfx/scribe-generator",
    "description": "Automatically generate Scribe-compatible PHPDoc comments for Laravel controllers by extracting validation rules from FormRequests and structure from JsonResources",
    "type": "library",
    "keywords": ["laravel", "scribe", "api", "documentation", "generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Varun Gupta",
            "email": "varun@webfixerr.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "WFX\\ScribeGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WFX\\ScribeGenerator\\ScribeGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}